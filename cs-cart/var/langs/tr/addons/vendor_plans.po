msgctxt "Addons::name::vendor_plans"
msgid "Vendor plans"
msgstr "Tedarikçi planları"

msgctxt "Addons::description::vendor_plans"
msgid "Create plans with different restrictions and prices for your vendors"
msgstr "Tedarikçileriniz için farklı kısıtlama ve fiyatlarla planlar oluşturun"

msgctxt "SettingsOptions::vendor_plans::commissions_header"
msgid "Commissions"
msgstr "Komisyonlar"

msgctxt "SettingsOptions::vendor_plans::include_shipping"
msgid "Include shipping cost in vendor commissions"
msgstr "Satıcı komisyonlarına sevkiyat maliyetini dahil et"

msgctxt "SettingsOptions::vendor_plans::include_taxes_in_commission"
msgid "Include taxes in vendor commissions"
msgstr "Vergileri satıcı komisyonlarına dahil et"

msgctxt "SettingsTooltips::vendor_plans::include_taxes_in_commission"
msgid "Calculate vendor commissions without subtracting taxes from the order total beforehand."
msgstr "Önceden sipariş toplamından vergi çıkarmadan satıcı komisyonlarını hesaplayın."

msgctxt "SettingsOptions::vendor_plans::allow_vendors_to_change_plan"
