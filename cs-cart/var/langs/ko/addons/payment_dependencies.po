msgctxt "Addons::name::payment_dependencies"
msgid "Payment dependencies"
msgstr "지불 종속성"

msgctxt "Addons::description::payment_dependencies"
msgid "Assign payment methods to shipping methods"
msgstr "배송 방법에 지불 방법 할당"

msgctxt "Languages::pd_allowed_payment_methods"
msgid "Allowed payment methods"
msgstr "허용된 결제 수단"

msgctxt "Languages::pd_allowed_payment_methods_text"
msgid "Select payment methods that your customers can use with this shipping method"
msgstr "고객이 이 배송 방법으로 사용할 수 있는 결제 방법을 선택하세요."

msgctxt "Languages::pd.there_are_no_available_payment_methods"
msgid "There are no payment methods available right now. Try to change the selected shipping method."
msgstr "현재 사용 가능한 결제 수단이 없습니다. 선택한 배송 방법을 변경해 보세요."

msgctxt "Languages::pd.no_payments_available"
msgid "No payment methods available."
msgstr "사용 가능한 결제 수단이 없습니다."

