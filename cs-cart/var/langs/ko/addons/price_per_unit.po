msgctxt "Addons::name::price_per_unit"
msgid "Price per unit"
msgstr "단위 당 가격"

msgctxt "Addons::description::price_per_unit"
msgid "Allows you to display price per unit on catalog and product pages."
msgstr "카탈로그 및 제품 페이지에 단위당 가격을 표시할 수 있습니다."

msgctxt "Languages::price_per_unit"
msgid "Price per unit"
msgstr "단위 당 가격"

msgctxt "Languages::unit_name"
msgid "Unit name"
msgstr "유닛명"

msgctxt "Languages::unit_name_field_description"
msgid "For example, 'ml' or 'g' without quotes."
msgstr "예를 들어 따옴표 없이 'ml' 또는 'g'입니다."

msgctxt "Languages::units_in_product"
msgid "Units in product"
msgstr "제품의 단위"

msgctxt "Languages::units_in_product_field_description"
msgid "Required for automatic calculation of price per unit."
msgstr "단위당 가격 자동 계산에 필요합니다."

msgctxt "Languages::show_price_per_x_units"
msgid "Show price per X units"
msgstr "X 단위당 가격 표시"

msgctxt "Languages::show_price_per_x_units_field_description"
msgid "For cases when you want to show price 'per 250 ml' instead of 'per ml'."
msgstr "'ml당' 대신 '250ml당' 가격을 표시하려는 경우."

