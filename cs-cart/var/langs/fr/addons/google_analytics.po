msgctxt "Addons::name::google_analytics"
msgid "Google Analytics"
msgstr "Google Analytics"

msgctxt "Addons::description::google_analytics"
msgid "Integrates a Google Analytics service with the store"
msgstr "Intègre un service Google Analytics avec le magasin"

msgctxt "SettingsSections::google_analytics::general"
msgid "General"
msgstr "Général"

msgctxt "SettingsOptions::google_analytics::tracking_code"
