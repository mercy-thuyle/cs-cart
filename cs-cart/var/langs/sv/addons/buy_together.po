msgctxt "Addons::name::buy_together"
msgid "Buy together [Deprecated]"
msgstr "Köp tillsammans [Deprecated]"

msgctxt "Addons::description::buy_together"
msgid "Allows you to set up share discounts on certain product combinations"
msgstr "Låter dig ställa in rabatter på vissa kombinationer av produkter"

msgctxt "Languages::buy_together"
msgid "Buy together"
msgstr "Köp tillsammans"

msgctxt "Languages::specify_options"
msgid "Specify options"
msgstr "Ange alternativ"

msgctxt "Languages::total_list_price"
msgid "Total list price"
msgstr "Totalt ord.pris"

msgctxt "Languages::price_for_all"
msgid "Price for all"
msgstr "Pris för alla"

msgctxt "Languages::add_all_to_cart"
msgid "Add all to cart"
msgstr "Lägg alla i varukorgen"

msgctxt "Languages::editing_combination"
msgid "Editing combination"
msgstr "Redigerar kombination"

msgctxt "Languages::buy_together_calculation_information"
msgid "Total price: [total_price], Discount: [discount], Combination price: [combination_price]"
msgstr "Summa: [total_price], Rabatt: [discount], Kombinerat pris: [combination_price]"

msgctxt "Languages::display_in_promotions"
msgid "Display in promotions"
msgstr "Visa i kampanjer"

msgctxt "Languages::combination_products"
msgid "Combination products"
msgstr "Kombinationsprodukter"

msgctxt "Languages::share_discount"
msgid "Share discount"
msgstr "Dela rabatt"

msgctxt "Languages::buy_together_is_not_compatible_with_configurator"
msgid "[product_name] cannot be added to combination. Buy together is not compatible with the Product configurator add-on"
msgstr "[product_name] kunde inte läggas till kombinationen. Köp tillsammans är inte kompatibel med tillägget Produktkonfigurator"

msgctxt "Languages::buy_together_is_not_compatible_with_recurring_billing"
msgid "[product_name] cannot be added to combination. Buy together is not compatible with the Recurring billing add-on"
msgstr "[product_name] kunde inte läggas till kombinationen. Köp tillsammans är inte kompatibel med tillägget Abonnemangsfakturering"

msgctxt "Languages::buy_together_combination_cannot_be_added"
msgid "Combination cannot be added to cart"
msgstr "Kombination kan inte läggas i varukorgen"

msgctxt "Languages::buy_together_product_was_removed"
msgid "[product] did not have enough amount ([amount]) and was removed from the cart"
msgstr "[Produkt] hade inte tillräckligt antal ([amount]) och togs bort från varukorgen"

msgctxt "Languages::buy_together_fill_the_mandatory_fields"
msgid "Please specify product options before adding this product combination to the cart."
msgstr "Ange produktalternativ innan du lägger till denna produktkombination i varukorgen."

msgctxt "Languages::buy_together_info_message_for_mve"
msgid "Only vendors can add product combinations. To add a combination, first <a class=\"buy-together-js-company-switcher\" data-ca-switcher-company-id=\"[company_id]\" data-ca-switcher-param-name=\"company_id\" data-ca-switcher-product-id=\"[product_id]\">log in as vendor</a>."
msgstr "Endast leverantörer kan lägga till produktkombinationer. För att lägga till en kombination, logga först <a class=\"buy-together-js-company-switcher\" data-ca-switcher-company-id=\"[company_id]\" data-ca-switcher-param-name=\"company_id\" data-ca-switcher-product-id=\"[product_id]\">in som leverantör</a>."

msgctxt "Languages::buy_together_info_message"
msgid "To add a combination, first <a class=\"buy-together-js-storefront-switcher\" data-ca-switcher-storefront-id=\"[storefront_id]\" data-ca-switcher-param-name=\"switch_company_id\" data-ca-switcher-product-id=\"[product_id]\">select the storefront</a>."
msgstr "För att lägga till en kombination, välj först <a class=\"buy-together-js-storefront-switcher\" data-ca-switcher-storefront-id=\"[storefront_id]\" data-ca-switcher-param-name=\"switch_company_id\" data-ca-switcher-product-id=\"[product_id]\">skyltfönster</a>."

