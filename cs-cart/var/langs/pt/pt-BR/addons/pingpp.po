msgctxt "Addons::name::pingpp"
msgid "Ping++ Payments"
msgstr "Pagamentos Ping++"

msgctxt "Addons::description::pingpp"
msgid "Lets you accept payments via Alipay, WeChat, UnionPay, Fqlpay, Baifubao, Yeepay, Jdpay and QGBC"
msgstr "Permite a voce aceitar pagamentos via Alipay,  Wechat, UnionPay, Fqlpay, Baifibao. Yeepay, Jdpay e QGBC"

msgctxt "Languages::pingpp.pingpp"
msgid "Ping++"
msgstr "Ping++"

msgctxt "Languages::pingpp.paying_with"
msgid "Paying with [channel]"
msgstr "Pagando com [channel]"

msgctxt "Languages::pingpp.enable_cny_currency"
msgid "To use the Ping++ payment method, you must have the Chinese yuan (CNY) currency enabled."
msgstr "Para usar o método de pagamento Ping++ , você deve ter a moeda  yuan  chinês (CNY) habilitado."

msgctxt "Languages::pingpp.general_settings"
msgid "General settings"
msgstr "Configurações gerais"

