msgctxt "Languages::vendor_plans.storefronts_update_for_plan.add_storefronts_message"
msgid "Add the vendor to the storefronts that got added to the plan|Add the vendors to the storefronts that got added to the plan"
msgstr "Lisää toimittaja suunnitelmaan lisättyihin julkisivuihin|Lisää toimittajat suunnitelmaan lisättyihin julkisivuihin"

msgctxt "Languages::vendor_plans.storefronts_update_for_plan.remove_storefronts_message"
msgid "Remove the vendor from the storefronts that got removed from this plan|Remove the vendors from the storefronts that got removed from this plan"
msgstr "Poista myyjä julkisivuista, jotka poistettiin tästä suunnitelmasta|Poista toimittajat julkisivuista, jotka poistettiin tästä suunnitelmasta"

msgctxt "Languages::vendor_plans.storefronts_update_for_vendor.title"
msgid "The vendor got moved to another plan"
msgstr "Myyjä siirtyi toiseen suunnitelmaan"

msgctxt "Languages::vendor_plans.storefronts_update_for_vendor.general_message"
msgid "What should we do about the storefronts that are available to this vendor?"
msgstr "Mitä meidän pitäisi tehdä tämän myyjän käytettävissä oleville julkisivuille?"

msgctxt "Languages::vendor_plans.storefronts_update_for_vendor.add_storefronts_message"
msgid "Add the vendor to the storefronts available in the new vendor plan"
msgstr "Lisää toimittaja uudessa toimittajasuunnitelmassa saatavilla oleviin julkisivuihin"

msgctxt "Languages::vendor_plans.storefronts_update_for_vendor.remove_storefronts_message"
msgid "Remove the vendor from the storefronts available in his previous plan"
msgstr "Poista myyjä aiemmassa suunnitelmassaan saatavilla olevista julkisivuista"

