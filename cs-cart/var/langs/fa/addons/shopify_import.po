msgctxt "Addons::name::shopify_import"
msgid "Import from Shopify"
msgstr "واردات از Shopify"

msgctxt "Addons::description::shopify_import"
msgid "Allows you to import products from Shopify."
msgstr "به شما امکان می دهد محصولات را از Shopify وارد کنید."

msgctxt "Languages::shopify_import.shopify_products_import"
msgid "Shopify products"
msgstr "محصولات Shopify"

msgctxt "Languages::shopify_import.sync_data_name"
msgid "Import products from Shopify"
msgstr "محصولات را از Shopify وارد کنید"

msgctxt "Languages::shopify_import.choose_import_mode"
msgid "Choose import mode"
msgstr "حالت واردات را انتخاب کنید"

msgctxt "Languages::shopify_import.import_for_all_vendors"
msgid "I'm launching my marketplace and setting up products for all vendors to use"
msgstr "من بازار خود را راه اندازی می کنم و محصولاتی را برای استفاده همه فروشندگان راه اندازی می کنم"

msgctxt "Languages::shopify_import.import_for_specific_vendor"
msgid "I'm importing these products for a specific vendor"
msgstr "من این محصولات را برای یک فروشنده خاص وارد می کنم"

msgctxt "Languages::shopify_import.depends_on_common_products_addon"
msgid "This option needs <a href=\"[link]\">Common products for vendors</a> add-on to be installed"
msgstr "این گزینه برای نصب به <a href=\"[link]\">محصول</a> برای فروشندگان نیاز دارد"

msgctxt "Languages::shopify_import.invalid_csv_file_format"
msgid "Uploaded CSV-file has invalid format or empty. Only files exported from Shopify supported."
msgstr "فایل CSV آپلود شده دارای قالب نامعتبر یا خالی است. فقط فایل های صادر شده از Shopify پشتیبانی می شوند."

msgctxt "Languages::shopify_import.products_without_sku"
msgid "The SKU value is missing for some products. These products are always imported as new objects."
msgstr "مقدار SKU برای برخی از محصولات وجود ندارد. این محصولات همیشه به عنوان اشیاء جدید وارد می شوند."

msgctxt "Languages::shopify_import.last_status.S"
msgid "Success"
msgstr "موفقیت"

msgctxt "Languages::shopify_import.last_status.F"
msgid "Failure"
msgstr "شکست"

