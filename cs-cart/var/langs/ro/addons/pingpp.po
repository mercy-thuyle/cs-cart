msgctxt "Addons::name::pingpp"
msgid "Ping++ Payments"
msgstr "Plăți Ping++"

msgctxt "Addons::description::pingpp"
msgid "Lets you accept payments via Alipay, WeChat, UnionPay, Fqlpay, Baifubao, Yeepay, Jdpay and QGBC"
msgstr "Vă permite să acceptați plăți prin Alipay, WeChat, UnionPay, Fqlpay, Baifubao, Yeepay, Jdpay și QGBC"

msgctxt "Languages::pingpp.pingpp"
msgid "Ping++"
msgstr "Ping++"

msgctxt "Languages::pingpp.paying_with"
msgid "Paying with [channel]"
msgstr "Plata cu [channel]"

msgctxt "Languages::pingpp.enable_cny_currency"
msgid "To use the Ping++ payment method, you must have the Chinese yuan (CNY) currency enabled."
msgstr "Pentru a utiliza metoda de plată Ping++, trebuie să aveți activată moneda yuanului chinezesc (CNY)."

msgctxt "Languages::pingpp.general_settings"
msgid "General settings"
msgstr "Setari generale"

msgctxt "Languages::pingpp.channels"
msgid "Payment channels"
msgstr "Canale de plată"

msgctxt "Languages::channel"
msgid "Channel"
msgstr "Canal"

msgctxt "Languages::pingpp.paid_at"
msgid "Paid at [time]"
msgstr "Plătit la [time]"

msgctxt "Languages::pingpp.channels.alipay_qr.instructions"
msgid "Open the <strong>Alipay</strong> application and scan the QR code to perform the payment. You will be redirected automatically after the payment will be received."
msgstr "Deschideți aplicația <strong>Alipay</strong> și scanați codul QR pentru a efectua plata. Veți fi redirecționat automat după ce plata va fi primită."

msgctxt "Languages::pingpp.channels.wx_pub_qr.instructions"
msgid "Open the <strong>WeChat</strong> application and scan the QR code to perform the payment. You will be redirected automatically after the payment will be received."
msgstr "Deschideți aplicația <strong>WeChat</strong> și scanați codul QR pentru a efectua plata. Veți fi redirecționat automat după ce plata va fi primită."

msgctxt "Languages::pingpp.channels.wx_pub.instructions"
msgid "Open this page in WeChat web browser to perform payment. You will be redirected automatically after the payment will be received."
msgstr "Deschideți această pagină în browserul web WeChat pentru a efectua plata. Veți fi redirecționat automat după ce plata va fi primită."

